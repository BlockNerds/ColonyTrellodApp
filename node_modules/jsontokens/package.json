    "web",
    "token",
    "encode",
    "decode",
    "verify",
    "ecdsa",
    "secp256k1",
    "ec",
    "elliptic",
    "curve",
    "signature",
    "sign"
  ],
  "author": "Blockstack PBC",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/blockstack/jsontokens-js/issues"
  },
  "homepage": "https://github.com/blockstack/jsontokens-js#readme",
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-preset-env": "^1.6.1",
    "babelify": "^7.3.0",
    "brfs": "^1.4.3",
    "browserify": "^13.1.1",
    "browserify-des": "^1.0.1",
    "tape": "^4.2.0"
  },
  "dependencies": {
    "asn1.js": "^4.9.1",
    "base64url": "^3.0.0",
    "elliptic": "^6.3.2",
