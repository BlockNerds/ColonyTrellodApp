{
  "name": "priorityqueue",
  "version": "0.2.1",
  "description": "An implementation of Priority Queue",
  "main": "lib/index.js",
  "scripts": {
    "build": "babel src --out-dir lib",
    "watch": "babel src --out-dir lib -w",
    "format": "esformatter -i src/**/*.js -i test/**/*.js",
    "prepublish": "npm run clean && npm run build",
    "clean": "rm -rf lib/*",
    "test": "mocha --compilers js:espower-babel/guess test/**/*.test.js"
  },
  "author": "berlysia",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:berlysia/priorityqueuejs.git"
  },
  "keywords": [
    "priority",
    "queue",
    "priority queue",
    "heap",
    "data structure"
  ],
  "devDependencies": {
    "babel": "^5.8.12",
    "esformatter": "^0.7.2",
    "espower-babel": "^3.2.0",
    "mocha": "^2.2.5",
    "power-assert": "^0.11.0",
    "pre-commit": "^1.0.10"
  },
  "pre-commit": [
    "format"
  ]
}
